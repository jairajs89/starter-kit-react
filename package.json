{
  "name": "starter-kit-react",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:jairajs89/starter-kit-react.git"
  },
  "engines": {
    "node": "4.2.1",
    "npm": "2.14.7"
  },
  "dependencies": {
    "zerver": "0.17.0"
  },
  "devDependencies": {
    "babel-core": "5.8.25",
    "eslint": "^1.7.1",
    "eslint-plugin-react": "^3.6.2",
    "jasmine-es6": "0.0.18",
    "jsdom": "^7.0.1",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "recess-jairaj": "^1.1.9"
  },
  "scripts": {
    "start": "zerver --babel --babel-exclude=xlib --cache 31536000,/index.jade:0 src",
    "deploy": "ZERVER_FLAGS='--s3-deploy=canvas.jairaj.org' npm start",
    "test": "npm run -s lint && npm run -s jasmine",
    "jasmine": "JASMINE_CONFIG_PATH=test/jasmine.json jasmine --stop-on-failure=true",
    "lint": "npm run -s lint-less && npm run -s lint-js",
    "lint-less": "recess src/style/*",
    "lint-js": "eslint --quiet --ignore-pattern=src/xlib/*.js src/*.js src/**/*.js test/*.js test/**/*.js"
  }
}

{
  "name": "starter-kit-react",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:jairajs89/starter-kit-react.git"
  },
  "engines": {
    "node": "4.2.1",
    "npm": "2.14.7"
  },
  "dependencies": {
    "zerver": "0.17.4"
  },
  "devDependencies": {
    "babel-core": "5.8.25",
    "eslint": "^1.7.1",
    "eslint-plugin-react": "^3.6.2",
    "jasmine-es6": "0.0.18",
    "jsdom": "^7.0.1",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "recess-jairaj": "1.1.10"
  },
  "scripts": {
    "start": "zerver --babel --babel-exclude=xlib --cache 31536000,/index.jade:0 src",
    "deploy": "npm test && ZERVER_FLAGS='--s3-deploy=mybucket' npm start",
    "test": "npm run -s lint && npm run -s test-js",
    "test-js": "JASMINE_CONFIG_PATH=package.json jasmine --stop-on-failure=true",
    "lint": "npm run -s lint-less && npm run -s lint-js",
    "lint-less": "recess src/style/*",
    "lint-js": "eslint --quiet --ignore-pattern=src/xlib/*.js src/*.js src/**/*.js test/*.js test/**/*.js"
  },
  "spec_dir": "test",
  "spec_files": [
      "*.js",
      "**/*.js"
  ],
  "recessConfig": {
    "includePath": ["src/style"],
    "noIDs": true,
    "noJSPrefix": true,
    "noOverqualifying": true,
    "noUnderscores": true,
    "noUniversalSelectors": true,
    "strictPropertyOrder": true,
    "zeroUnits": true,
    "prefixWhitespace": false,
    "noSummary": true
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true,
      "worker": true,
      "es6": true,
      "jasmine": true
    },
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-cond-assign": [
        2,
        "always"
      ],
      "no-control-regex": 2,
      "no-debugger": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-empty": 2,
      "no-ex-assign": 2,
      "no-extra-boolean-cast": 2,
      "no-extra-parens": 2,
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-inner-declarations": [
        2,
        "both"
      ],
      "no-invalid-regexp": 2,
      "no-irregular-whitespace": 2,
      "no-negated-in-lhs": 2,
      "no-obj-calls": 2,
      "no-regex-spaces": 2,
      "no-sparse-arrays": 2,
      "no-unexpected-multiline": 2,
      "no-unreachable": 2,
      "use-isnan": 2,
      "valid-typeof": 2,
      "block-scoped-var": 2,
      "consistent-return": 2,
      "curly": [
        2,
        "all"
      ],
      "dot-location": [
        2,
        "property"
      ],
      "dot-notation": [
        2,
        {
          "allowKeywords": false
        }
      ],
      "eqeqeq": 2,
      "no-alert": 2,
      "no-caller": 2,
      "no-div-regex": 2,
      "no-empty-pattern": 2,
      "no-eq-null": 2,
      "no-eval": 2,
      "no-extend-native": 2,
      "no-extra-bind": 2,
      "no-fallthrough": 2,
      "no-floating-decimal": 2,
      "no-implicit-coercion": 2,
      "no-implied-eval": 2,
      "no-invalid-this": 2,
      "no-iterator": 2,
      "no-labels": 2,
      "no-lone-blocks": 2,
      "no-loop-func": 2,
      "no-magic-numbers": 2,
      "no-multi-spaces": [
        2,
        {
          "exceptions": {
            "Property": true,
            "VariableDeclarator": true,
            "ImportDeclaration": true
          }
        }
      ],
      "no-multi-str": 2,
      "no-native-reassign": 2,
      "no-new-func": 2,
      "no-new-wrappers": 2,
      "no-octal-escape": 2,
      "no-octal": 2,
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ],
      "no-process-env": 2,
      "no-proto": 2,
      "no-redeclare": [
        2,
        {
          "builtinGlobals": true
        }
      ],
      "no-return-assign": [
        2,
        "always"
      ],
      "no-script-url": 2,
      "no-self-compare": 2,
      "no-sequences": 2,
      "no-throw-literal": 2,
      "no-useless-call": 2,
      "no-useless-concat": 2,
      "no-void": 2,
      "no-warning-comments": [
        2,
        {
          "terms": [
            "todo",
            "fixme"
          ],
          "location": "start"
        }
      ],
      "no-with": 2,
      "wrap-iife": [
        2,
        "inside"
      ],
      "strict": [
        2,
        "never"
      ],
      "no-catch-shadow": 2,
      "no-delete-var": 2,
      "no-shadow-restricted-names": 2,
      "no-undef-init": 2,
      "no-undef": [
        2,
        {
          "typeof": true
        }
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "local",
          "args": "all",
          "varsIgnorePattern": "React"
        }
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "array-bracket-spacing": [
        2,
        "never"
      ],
      "block-spacing": [
        2,
        "always"
      ],
      "brace-style": [
        2,
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "camelcase": [
        2,
        {
          "properties": "always"
        }
      ],
      "comma-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "comma-style": [
        2,
        "last"
      ],
      "computed-property-spacing": [
        2,
        "never"
      ],
      "consistent-this": [
        2,
        "self"
      ],
      "eol-last": 2,
      "func-style": [
        2,
        "declaration",
        {
          "allowArrowFunctions": true
        }
      ],
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "key-spacing": [
        2,
        {
          "align": "colon",
          "beforeColon": false,
          "afterColon": true
        }
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "new-parens": 2,
      "no-array-constructor": 2,
      "no-lonely-if": 2,
      "no-mixed-spaces-and-tabs": [
        2,
        "smart-tabs"
      ],
      "no-negated-condition": 2,
      "no-nested-ternary": 2,
      "no-new-object": 2,
      "no-spaced-func": 2,
      "no-trailing-spaces": [
        2,
        {
          "skipBlankLines": false
        }
      ],
      "no-unneeded-ternary": [
        2,
        {
          "defaultAssignment": false
        }
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "one-var": [
        2,
        "never"
      ],
      "operator-assignment": [
        2,
        "always"
      ],
      "operator-linebreak": [
        2,
        "before"
      ],
      "padded-blocks": [
        2,
        "never"
      ],
      "quote-props": [
        2,
        "as-needed",
        {
          "keywords": true
        }
      ],
      "quotes": [
        2,
        "single"
      ],
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "semi": [
        2,
        "always"
      ],
      "space-after-keywords": [
        2,
        "always"
      ],
      "space-before-blocks": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "never"
        }
      ],
      "space-before-keywords": [
        2,
        "always"
      ],
      "space-in-parens": [
        2,
        "never"
      ],
      "space-infix-ops": 2,
      "space-return-throw-case": 2,
      "space-unary-ops": 2,
      "arrow-parens": [
        2,
        "always"
      ],
      "arrow-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "constructor-super": 2,
      "generator-star-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "no-class-assign": 2,
      "no-const-assign": 2,
      "no-dupe-class-members": 2,
      "no-var": 2,
      "object-shorthand": [
        2,
        "always"
      ],
      "prefer-arrow-callback": 2,
      "prefer-const": 2,
      "prefer-spread": 2,
      "prefer-template": 2,
      "require-yield": 2
    }
  }
}
